{
  "name": "cognito-service-app",
  "description": "JavaScript service for AWS Cognito with Angular 6",
  "version": "1.0.1",
  "author": "Nicolas Roehm <nicolas.roehm@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "authentication",
    "aws",
    "cognito",
    "facebook",
    "google",
    "angular",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Caliatys/CognitoService.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bar": "ng build --prod cognito-service && ng s --port=4300",
    "build_lib": "ng build --prod cognito-service",
    "npm_pack": "cd dist/cognito-service && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "npm_publish": "npm run package && cd dist/cognito-service && npm publish --access=public"
  },
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.2.1",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@caliatys/login-form": "^2.0.2",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/gapi": "0.0.35",
    "@types/gapi.auth2": "0.0.47",
    "amazon-cognito-identity-js": "2.0.6",
    "amazon-cognito-js": "1.1.0",
    "aws-sdk": "2.247.1",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "rxjs": "6.1.0",
    "rxjs-compat": "^6.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.3",
    "@angular-devkit/build-ng-packagr": "~0.6.6",
    "@angular-devkit/build-angular": "~0.6.5",
    "ng-packagr": "^3.0.0-rc.2",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.5",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
